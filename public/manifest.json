{
  "manifest_version": 3,
  "name": "Tab Manager",
  "version": "1.0",
  "description": "Description",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "logo/16.png",
      "32": "logo/32.png",
      "64": "logo/64.png",
      "128": "logo/128.png",
      "256": "logo/256.png"
    }
  },
  "permissions": ["activeTab", "storage"],
  "icons": {
    "16": "logo/16.png",
    "32": "logo/32.png",
    "64": "logo/64.png",
    "128": "logo/128.png",
    "256": "logo/256.png"
  }
}
